<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="@dimen/dimen10dp"
    android:layout_marginEnd="@dimen/dimen10dp">
<!--    row 1-->
<!--    LatD-->
    <TextView
        android:id="@+id/tvLatD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LatD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
<!--    LatM -->
    <TextView
        android:id="@+id/tvLatM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LatM"
        app:layout_constraintEnd_toStartOf="@id/tvLatS"
        app:layout_constraintStart_toEndOf="@id/tvLatD"
        app:layout_constraintTop_toTopOf="parent" />
<!--    LatS -->
    <TextView
        android:id="@+id/tvLatS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LatS"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
<!--    row 2-->
<!--    NS-->
    <TextView
        android:id="@+id/tvNs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvLatD" />
    <!--    LonD-->
    <TextView
        android:id="@+id/tvLonD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LonD"
        app:layout_constraintend_toEndOf="@id/tvLatM"
        app:layout_constraintStart_toStartOf="@id/tvLatM"
        app:layout_constraintTop_toBottomOf="@id/tvLatM" />
    <!--    LonM-->
    <TextView
        android:id="@+id/tvLonM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LonM"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvLatS" />
<!--    row 3-->
    <!--    LonS-->
    <TextView
        android:id="@+id/tvLonS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LonS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvNs" />
<!--    EW-->
    <TextView
        android:id="@+id/tveW"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EW"
        app:layout_constraintend_toEndOf="@id/tvLatM"
        app:layout_constraintStart_toStartOf="@id/tvLatM"
        app:layout_constraintTop_toBottomOf="@id/tvLonD" />
<!--    City-->
    <TextView
        android:id="@+id/tvCity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvLonM" />
<!--    row 4-->
<!--    State-->
    <TextView
        android:id="@+id/tvState"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="State"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvLonS"
        android:layout_marginBottom="@dimen/dimen_15dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>